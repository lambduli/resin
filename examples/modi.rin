theorem trivial: A, B ⊢ B.


theorem modus-ponens: ∃ x P(x) ∧ Q(x)
                    , ∀ x P(x) ∧ Q(x) => K(x)
                    ⊢ ∃ x K(x) .


theorem prop-modus-tollens: A => B
                          , ¬B
                          ⊢ ¬A .


theorem fol-modus-tollens : ∀ x A(x) => B(x)
                          , ∀ y ¬B(y)
                          ⊢ ∀ z ¬A(z) .


